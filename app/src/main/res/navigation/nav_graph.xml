<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/listPriceFragment">

    <fragment
        android:id="@+id/listPriceFragment"
        android:name="com.example.testefishery.ui.listprice.ListPriceFragment"
        android:label="fragment_list_price"
        tools:layout="@layout/fragment_list_price" >
        <action
            android:id="@+id/action_listPriceFragment_to_addPriceFragment"
            app:destination="@id/addPriceFragment" />
        <action
            android:id="@+id/action_listPriceFragment_to_formFilterFragment"
            app:destination="@id/formFilterFragment" />
    </fragment>

    <fragment
        android:id="@+id/addPriceFragment"
        android:name="com.example.testefishery.ui.addprice.AddPriceFragment"
        android:label="fragment_add_price"
        tools:layout="@layout/fragment_add_price" />

    <fragment
        android:id="@+id/formFilterFragment"
        android:name="com.example.testefishery.ui.formfilter.FormFilterFragment"
        android:label="fragment_formfilter"
        tools:layout="@layout/fragment_formfilter" />
</navigation>